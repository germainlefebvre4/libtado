{
  "$schema": "http://json-schema.org/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "dayType": {
        "type": "string"
      },
      "start": {
        "type": "string"
      },
      "end": {
        "type": "string"
      },
      "geolocationOverride": {
        "type": "boolean"
      },
      "setting": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string"
          },
          "power": {
            "type": "string"
          },
          "temperature": {
            "type": "object",
            "properties": {
              "celsius": {
                "type": "number"
              },
              "fahrenheit": {
                "type": "number"
              }
            },
            "required": [
              "celsius",
              "fahrenheit"
            ]
          }
        },
        "required": [
          "power",
          "temperature",
          "type"
        ]
      }
    },
    "required": [
      "dayType",
      "end",
      "geolocationOverride",
      "setting",
      "start"
    ]
  }
}
